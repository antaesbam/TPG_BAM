POST _ingest/pipeline/_simulate
{
  "pipeline" :
  {
    "description": "tpg csv ingestion",
    "processors": [
      {
        "grok": {
         "field": "message",
         "patterns": [
           "%{TIMESTAMP_TPG:date},%{DATA:phone},%{DATA:theme},\"%{DATA:cost}\",%{DATA:currency},%{WORD:delivery_status}([|]%{DELIVERY_ERROR_CODE_TPG:error_code})?([|]%{DELIVERY_ERROR_REASON_TPG:error_reason})?([|]%{DELIVERY_ERROR_MESSAGE_TPG:error_message})?,%{WORD:operator}"
         ],
         "pattern_definitions": {
          "SECOND_TPG": "(?:(?:[0-5]?[0-9]|60)(?:[:.,][0-9]+)?)[+-](0[0-9])?",
          "TIMESTAMP_TPG": "(%{YEAR}-%{MONTHNUM}-%{MONTHDAY}[T ]%{HOUR}:?%{MINUTE}(?::?%{SECOND_TPG}))?",
          "DELIVERY_STATUS_TPG": "((%{WORD})([|]?(%{WORD}[ -]?))*)?",
          "DELIVERY_ERROR_CODE_TPG": "%{INT}",
          "DELIVERY_ERROR_MESSAGE_TPG": "(%{WORD}[ -]+)+",
          "DELIVERY_ERROR_REASON_TPG": "rejected"
        }
       }
      },
      {
        "remove": {
          "field": "message"
        }
      },
      {
        "set": {
          "field": "_type",
          "value": "tpg_sms"
        }
      },
      {
        "set": {
          "field": "_index",
          "value": "tpg_mobile_tickets"
        }
      }
    ]
  },
